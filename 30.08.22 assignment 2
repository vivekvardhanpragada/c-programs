#include<stdio.h>
#include<string.h>
int main()
{
    char str[1000];
    printf("enter the string:");
    fgets(str,1000,stdin);
    for(int i = 0; i < strlen(str); i++)
    {
        if(str[i] == 'a')
        {
            printf("A");
        }
        else
        {
            printf("%c", str[i]);
        }
    }
}
