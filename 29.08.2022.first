#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
//Complete the following function.


int main() {
    int n, a, b, c,i;
    scanf("%d",&n);
    int s[n];
    scanf("%d %d %d",&a,&b,&c);
    s[0]=a;
    s[1]=b;
    s[2]=c;
    for(i=3;i<n;i++)
    {
      s[i]=s[i-1]+s[i-2]+s[i-3];  
    } 
    printf("%d",s[n-1]);
   
    return 0;
}
